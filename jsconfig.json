// jsconfig.json
{
  "compilerOptions": {
    "target": "es2017", // Ou uma vers√£o mais recente, como "es2020"
    "module": "commonjs",
    "types": [
      "cypress" // Isso informa ao VS Code para incluir os tipos do Cypress
    ]
  },
  "include": [
    "**/*.js",
    "**/*.cy.js", // Certifique-se de incluir seus arquivos de teste
    "cypress/**/*.js"
  ],
  "exclude": [
    "node_modules"
  ]
}